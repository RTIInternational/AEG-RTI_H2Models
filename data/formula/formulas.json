    {
      "name": "total_remaining_depreciation_charges",
      "expression": "sum(remaining_depreciation_charges)"
    },
    {
      "name": "depreciation_charges_column",
      "label": "Depreciation Charges Column",
      "expression": "get_depreciation_charges_column(depreciation_charges, analysis_index_range, total_remaining_depreciation_charges, anal_period, construct)",
      "excel_loc": "Cash Flow Analysis U64"
    },
    {
      "name": "discounted_value_depreciation_charges",
      "label": "Discounted Value of Depreciation Charges",
      "expression": "get(depreciation_charges_column, YEAR_1) + npv(nominal_irr, skip(depreciation_charges_column, 1))",
      "excel_loc": "Cash Flow Analysis U116"
    },
    {
      "name": "pv_of_cashflow_depreciation_charges",
      "label": "PV of Cash Flow of Depreciation Charges",
      "expression": "npv(nominal_irr, depreciation_charges_column)",
      "excel_loc": "Cash Flow Analysis U115"
    },
    {
      "name": "LCOE_contribution_depreciation",
      "label": "LCOE Contribution from Depreciation",
      "expression": "discounted_value_depreciation_charges * total_tax_rate",
      "excel_loc": "Cash Flow Analysis W57"
    },
    {
      "name": "LCOE_contribution_principal_payments",
      "label": "Discounted Value of Depreciation Charges",
      "expression": "-(get(principal_payments_column, YEAR_1) + npv(nominal_irr, skip(principal_payments_column, 1)))",
      "excel_loc": "Cash Flow Analysis Z116"
    },
    {
      "name": "pv_of_cashflow_principal_payments",
      "label": "PV of Cash Flow of Pricipal Payments",
      "expression": "npv(nominal_irr, principal_payments_column)",
      "excel_loc": "Cash Flow Analysis Z115"
    },
    {
      "name": "LCOE_contribution_operation_costs",
      "label": "LCOE Contribution from Operation Costs",
      "expression": "-(discounted_value_total_salvage_value + discounted_value_decom_costs + discounted_value_fixed_cost + discounted_value_feedstock_cost + discounted_value_other_raw_material_cost + discounted_value_variable_cost + discounted_value_interest_payments) * (1-total_tax_rate)",
      "excel_loc": "Cash Flow Analysis AA57"
    },
    {
      "orig_name": "H2_price_nominal",
      "label": "Hydrogen Cost (Start-up Year Nominal $/kg)",
      "expression": "((LCOE_contribution_capital_costs + LCOE_contribution_depreciation + LCOE_contribution_principal_payments + LCOE_contribution_operation_costs) / LCOE_contribution_h2_sales_kg) * (1 + inflation_rate) ** construct",
      "excel_loc": "Cash Flow Analysis E17"
    },
    {
      "orig_name": "H2_price_real",
      "label": "Hydrogen Cost (Ref Year Dollars per kg)",
