{
  "formulas": [
    {
      "name": "plant_output_kg_per_day",
      "label": "Plant Output (kg/day)",
      "expression": "plant_design_capacity * capacity_factor",
      "orig_name": "output_kgpday",
      "excel_loc": "Input_Sheet_Template, C27"
    },
    {
      "name": "plant_output_kg_per_year",
      "label": "Plant Output (kg/year)",
      "expression": "plant_output_kg_per_day * 365",
      "orig_name": "output_kgyear",
      "excel_loc": "Input_Sheet_Template, C28"
    },
    {
      "name": "full_analysis_period",
      "label": "",
      "expression": "construct + anal_period",
      "orig_name": ""
    },
    {
      "name": "analysis_period_start",
      "label": "",
      "expression": "startup_year - construct",
      "orig_name": ""
    },
    {
      "name": "analysis_period_end",
      "label": "",
      "expression": "analysis_period_start + full_analysis_period",
      "orig_name": ""
    },
    {
      "name": "",
      "label": "Chemical Engineering Plant Cost Index (CEPCI) Inflator",
      "expression": "get_plant_cost_index(CurrentYear) / get_plant_cost_index(BasisYear)",
      "orig_name": "CEPCIinflator",
      "excel_loc": "Capital Costs, C11"
    },
    {
      "name": "",
      "label": "Consumer Price Inflator (CPI)",
      "expression": "get_cpi(ref_year) / get_cpi(CurrentYear)",
      "orig_name": "CPIinflator",
      "excel_loc": "Capital Costs, C12"
    },
    {
      "name": "",
      "label": "INFLATION FACTOR",
      "expression": "(1 + inflation_rate) ** (startup_year - ref_year)",
      "orig_name": "INFLATION_FACTOR",
      "excel_loc": "Cash Flow Analysis, B128"
    },
    {
      "name": "",
      "label": "Hydrogen Gas Lower Heating Value (LHV) (MJ/kg)",
      "expression": "get_lhv(\"Hydrogen\")",
      "orig_name": "H2_LHV_MJ_p_kg",
      "excel_loc": "HyARC Physical Property Data, E7"
    },
    {
      "name": "output_energy_MMBtu_per_year",
      "label": "Actual Hydrogen Energy Produced (MMBtu(LHV)/yr)",
      "expression": "plant_output_kg_per_year * H2_LHV_MJ_p_kg / 1000 / conversion_factor(\"mmBTU_to_GJ\")",
      "orig_name": ""
    },
    {
      "name": "output_energy_MJ_per_year",
      "label": "Actual Hydrogen Energy Produced (MJ(LHV)/yr)",
      "expression": "plant_output_kg_per_year * H2_LHV_MJ_p_kg",
      "orig_name": ""
    },
    {
      "name": "",
      "label": "After-Tax Nominal IRR",
      "expression": "((1 + real_irr) * (1 + inflation_rate)) - 1",
      "orig_name": "nominal_irr",
      "excel_loc": "Cash Flow Analysis, B27"
    },
    {
      "name": "percentage_debt_financing",
      "label": "Debt Financing",
      "expression": "1 - percentage_equity_financing",
      "orig_name": "",
      "excel_loc": "Cash Flow Analysis, B31"
    },
    {
      "name": "target_after_tax_nominal_irr",
      "label": "Target after-tax nominal IRR",
      "expression": "(1 + real_irr)*(1 + inflation_rate) - 1",
      "orig_name": "",
      "excel_loc": "Cash Flow Analysis, X59"
    },
    {
      "name": "analysis_range",
      "expression": "range(analysis_period_start, analysis_period_end)",
      "sample": "range(2012, 2055)"
    },
    {
      "name": "analysis_index_range",
      "expression": "seq_along(analysis_range)",
      "sample": "range(0, 43)"
    },
    {
      "name": "feedstock_price_df",
      "expression": "get_feedstock_price_df(feedstocks, analysis_range, startup_year, INFLATION_FACTOR)",
      "excel_loc": "Cash Flow Analysis B139:E181"
    },
    {
      "name": "operation_range",
      "expression": "get_operation_range(analysis_index_range, construct)",
      "sample": "[-3, -2, -1, 1, 2, 3, ..., 40]"
    },
    {
      "name": "inflation_price_increase_factors",
      "expression": "get_inflation_price_increase_factors(analysis_range, inflation_rate, startup_year)",
      "excel_loc": "Cash Flow Analysis, E65:E107"
    },
    {
      "name": "total_feedstock_cost_column",
      "label": "Feedstock Cost column",
      "expression": "get_total_feedstock_costs(operation_range, feedstock_price_df, inflation_price_increase_factors, start_time, plant_output_kg_per_year, percnt_var)",
      "excel_loc": "Cash Flow Analysis, O65:O107"
    },
    {
      "name": "discounted_value_feedstock_cost",
      "label": "Discounted Value of Feedstock Cost",
      "expression": "get(total_feedstock_cost_column, YEAR_1) + npv(target_after_tax_nominal_irr, skip(total_feedstock_cost_column, 1))",
      "excel_loc": "Cash Flow Analysis, O116"
    },
    {
      "orig_name": "direct_cap",
      "label": "H2A Total Direct Capital Cost",
      "expression": "sum(capital_investment_costs(capital_investments, CEPCIinflator, CPIinflator))",
      "excel_loc": "Input_Sheet_Template, C88"
    },
    {
      "orig_name": "CO2_seq",
      "label": "H2A Carbon Sequestration Total Direct Capital Cost",
      "expression": "0",
      "excel_loc": "Input_Sheet_Template C89",
      "translation_note": "Ironically, this value is only in no-cc model and ignored in the with-cc model"
    },
    {
      "name": "site_preparation_cost",
      "label": "Site Preparation ($)",
      "expression": "evaluate(site_preparation)",
      "excel_loc": "Input_Sheet_Template, C92",
      "translation_note": "The formula is 2 lines in the formula bar in Excel"
    },
    {
      "name": "engineering_and_design_cost",
      "label": "Engineering and Design ($)",
      "expression": "evaluate(engineering_and_design)",
      "excel_loc": "Input_Sheet_Template, C93"
    },
    {
      "name": "process_contingency_cost",
      "label": "Process Contingency ($)",
      "expression": "evaluate(process_contingency)",
      "excel_loc": "Input_Sheet_Template, C94"
    },
    {
      "name": "project_contingency_cost",
      "label": "Project Contingency ($)",
      "expression": "evaluate(project_contingency)",
      "excel_loc": "Input_Sheet_Template, C95"
    },
    {
      "name": "upfront_permitting_costs",
      "label": "Upfront Permitting Costs ($)",
      "expression": "evaluate(upfront_permitting)",
      "excel_loc": "Input_Sheet_Template, C98"
    },
    {
      "orig_name": "depr_cap",
      "label": "Total Depreciable Capital Costs",
      "expression": "direct_cap + CO2_seq + (CEPCIinflator * CPIinflator) * (site_preparation_cost + engineering_and_design_cost + process_contingency_cost + project_contingency_cost + other_depreciable_capital_cost + upfront_permitting_costs)",
      "excel_loc": "Input_Sheet_Template, E99"
    },
    {
      "name": "replacement_costs",
      "label": "Yearly Replacement Costs Column",
      "expression": "get_replacement_costs(operation_range, inflation_price_increase_factors, replace_factor, depr_cap, ref_year, startup_year, inflation_rate)",
      "excel_loc": "Cash Flow Analysis, I65:I107"
    },
    {
      "name": "discounted_value_replacement_costs",
      "label": "Discounted Value of Yearly Replacement Costs",
      "expression": "get(replacement_costs, YEAR_1) + npv(nominal_irr, skip(replacement_costs, 1))",
      "excel_loc": "Cash Flow Analysis, I116"
    },
    {
      "name": "pv_of_cashflow_replacement_costs",
      "label": "PV of Cash Flow of Yearly Replacement Costs",
      "expression": "npv(nominal_irr, replacement_costs)",
      "excel_loc": "Cash Flow Analysis, I115"
    },
    {
      "name": "land_cost",
      "label": "Land Cost ($)",
      "expression": "acres_required * cost_per_acre * CPIinflator",
      "excel_loc": "Input_Sheet_Template, E104"
    },
    {
      "name": "other_non_depreciable_capital_cost",
      "label": "Other Non-Depreciable Capital ($)",
      "expression": "0",
      "excel_loc": "Input_Sheet_Template, E105"
    },
    {
      "name": "non_dep_cap",
      "label": "Non-Depreciable Capital Costs",
      "expression": "land_cost + other_non_depreciable_capital_cost",
      "excel_loc": "Input_Sheet_Template, E106"
    },
    {
      "name": "total_cap",
      "label": "Total Capital Costs",
      "expression": "depr_cap + non_dep_cap",
      "excel_loc": "Input_Sheet_Template, C108"
    },
    {
      "orig_name": "depr_cap_infl",
      "label": "Total Depreciable Capital Costs",
      "expression": "(total_cap - non_dep_cap) * INFLATION_FACTOR",
