      "label": "Recovery Range",
      "expression": "range(1, 22)",
      "excel_loc": "Depreciation C48-W48"
    },
    {
      "name": "depreciation_calculation_table",
      "label": "Depreciation Calculation Table",
      "expression": "get_depreciation_calculation_table(recovery_range, operation_range, depr_type, depr_length, annual_depreciable_capital)",
      "excel_loc": "Depreciation C49"
    },
    {
      "name": "recovery_index_range",
      "expression": "seq_along(recovery_range)"
    },
    {
      "name": "depreciation_charges",
      "label": "Depreciation Charges",
      "expression": "get_depreciation_charges(analysis_index_range, recovery_index_range, depreciation_calculation_table)",
      "excel_loc": "Depreciation B97"
    },
    {
      "name": "remaining_depreciation_range",
      "expression": "range(anal_period+construct, anal_period+construct+depr_length+1)"
    },
    {
      "name": "remaining_depreciation_charges",
      "label": "Remaining Depreciation Charges",
      "expression": "get_depreciation_charges(remaining_depreciation_range, recovery_index_range, depreciation_calculation_table)",
      "excel_loc": "Depreciation B140 B146-B165"
