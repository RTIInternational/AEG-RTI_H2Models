      "excel_loc": "Cash Flow Analysis AC64"
    },
    {
      "orig_name": "after_tax_nominal_IRR",
      "label": "After-Tax Nominal IRR",
      "expression": "irr(aftertax_post_depreciation_cashflow_column)",
      "excel_loc": "Cash Flow Analysis E20"
